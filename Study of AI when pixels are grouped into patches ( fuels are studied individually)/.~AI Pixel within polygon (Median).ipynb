    "def scatter_hist(x, y, ax, ax_histx, ax_histy):\n",
    "    # no labels\n",
    "    ax_histx.tick_params(axis=\"x\", labelbottom=False)\n",
    "    ax_histy.tick_params(axis=\"y\", labelleft=False)\n",
    "  \n",
    "    # the scatter plot:\n",
    "    ax.scatter(x, y, color = 'c', edgecolor = 'black', marker='.')\n",
