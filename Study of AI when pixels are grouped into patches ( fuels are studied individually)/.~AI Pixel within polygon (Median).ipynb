   "execution_count": 2,
   "id": "cf929ade",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import rasterio\n",
    "import geopandas as gpd\n",
    "from shapely.geometry import Point\n",
    "from rasterio.crs import CRS\n",
    "\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
