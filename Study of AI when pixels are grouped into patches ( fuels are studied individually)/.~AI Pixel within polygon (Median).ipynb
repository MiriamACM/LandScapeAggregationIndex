   "source": [
    "# This code is to calculate the ratio between the Area of the polygon and the related patch's Area\n",
    "\n",
    "def ratio (df):\n",
    "    \n",
    "\n",
    "    df['Ratio (%)'] = df.apply(lambda x: (((x['Area_Ha']) - x['Area_Ha (patch)'])/(x['Area_Ha']))*100, axis=1)\n",
    "\n",
    "    \n",
    "    return df"
   ]
